<div class="notice-wrapper">
   <div class="row">
     <% if flash[:notice] %>
           <p class="flash-notice">
             <%= flash[:notice] %>
           </p>
       <% end %>
        <% if flash[:error]%>
            <p class="flash-error">
              <%= flash[:error] %>
            </p>
     <% end%>
   </div>
</div>

